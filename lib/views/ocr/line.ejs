<% var bbox = line.attributes.title.split(' '); %>
<canvas class="ocr_line panel panel-default" width="<%= 1*bbox[3]-bbox[1]*1 %>" height="<%= 1*bbox[4]-bbox[2]*1 %>" id="<%= line.attributes.title %>"></canvas>
<div class="hocr_line">
<div class="btn-group" id="<%= line.attributes.title %>">
    <% for (j in line.span()) { if (j!='last') { var word = line.span(j); %>
    <% include word %>
    <% }} %>
</div>
</div>