<script type="text/javascript">
    // Word-level indexing of de-serialized jsOCR
    function TOC(index,elem) {
        if (elem.attributes && elem.attributes.id) index[elem.attributes.id] = elem;
        if (elem.children) elem.children.forEach(function(e){TOC(index,e)});
    }
    var json = <%- JSON.stringify(json) %>;
    TOC(json.index,json.doc.html)
</script>